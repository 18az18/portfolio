{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "portfolio of Tian Ji Liu",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.client.js && webpack --config webpack.server.js",
    "dev:build-server": "webpack --config webpack.server.js --mode=development --watch",
    "dev:server": "nodemon ./server",
    "dev": "npm-run-all --parallel build dev:*",
    "prod": "npm run build && node build/bundle.js",
    "start": "node ./server/index.js",
    "heroku-postbuild": "npm install && npm run prod"
  },
  "author": "Tian Ji Liu",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "babel-loader": "^8.2.3",
    "cors": "^2.8.5",
    "css-loader": "^6.5.1",
    "express": "^4.17.1",
    "isomorphic-style-loader": "^5.3.2",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
